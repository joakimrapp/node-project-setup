{
  "name": "@jrapp/node-project-setup",
  "version": "1.0.0",
  "description": "Node project setup",
  "main": "./src/index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --reporter spec --colors --timeout 10000 --async-only --recursive ./test/tests",
    "lint": "./node_modules/.bin/eslint ./src",
    "coverage": "./node_modules/istanbul/lib/cli.js cover --config ./test/istanbul/report.yml ./node_modules/.bin/_mocha -- --recursive ./test/tests --timeout 10000 --async-only --reporter xunit --reporter-options output=./ci/reports/coverage/test.xml",
    "test-report": "npm -s run lint && npm -s run test && npm -s run coverage"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:joakimrapp/node-project-setup.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joakimrapp/node-project-setup/issues"
  },
  "homepage": "https://github.com/joakimrapp/node-project-setup#readme",
  "dependencies": {
    "eslint": "3.19.0",
    "istanbul": "0.4.5",
    "mocha": "3.3.0",
    "stack-trace": "0.0.9"
  }
}
